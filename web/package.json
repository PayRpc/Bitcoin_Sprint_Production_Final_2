{
  "name": "bitcoin-sprint-web",
  "version": "2.1.0",
  "description": "Multi-Chain Sprint Enterprise Web Dashboard and API - Supporting Bitcoin, Ethereum, and Solana",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx next dev -p 3000",
    "dev:free": "npx next dev -p 3000",
    "dev:business": "npx next dev -p 3001",
    "dev:enterprise": "npx next dev -p 3002",
    "dev:memory": "node --max-old-space-size=8192 npx next dev -p 3000",
    "dev:memory:business": "node --max-old-space-size=8192 npx next dev -p 3001",
    "dev:memory:enterprise": "node --max-old-space-size=8192 npx next dev -p 3002",
    "build": "npx next build",
    "start": "npx next start -p 3000",
    "start:free": "npx next start -p 3000",
    "start:business": "npx next start -p 3001",
    "start:enterprise": "npx next start -p 3002",
    "lint": "npx next lint",
    "typecheck": "npx tsc --noEmit",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "maintenance": "node scripts/maintenance.js",
    "maintenance:status": "node scripts/maintenance.js status",
    "maintenance:enable": "node scripts/maintenance.js enable",
    "maintenance:disable": "node scripts/maintenance.js disable",
    "maintenance:health": "node scripts/maintenance.js health",
    "reset:daily": "node scripts/reset-daily-counters.js",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "performance:test": "node scripts/performance-test.js",
    "compliance:check": "node scripts/compliance-check.js",
    "setup:dev": "node setup-dev.js",
    "test:connection": "node test-connection.js",
    "test:server": "node test-server.js",
    "test:entropy": "node test-entropy-auth.js",
    "test:entropy:web": "node test-entropy-web-api.js",
    "demo:entropy": "node demo-entropy.js",
    "test:monitor": "node test-entropy-monitor.js",
    "test:monitor:standalone": "node test-entropy-monitor-standalone.js",
    "test:all": "npm run test:entropy && npm run test:entropy:web && npm run test:monitor && npm run test:server && npm run test:connection"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^11.0.0",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.400.0",
    "next": "^14.2.7",
    "node-cache": "^5.1.2",
    "pino": "^8.21.0",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "20.19.13",
    "@types/react": "18.3.24",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "bitcoin",
    "sprint",
    "nextjs",
    "prisma",
    "tailwind",
    "api"
  ],
  "author": "BitcoinCab.inc",
  "license": "MIT"
}